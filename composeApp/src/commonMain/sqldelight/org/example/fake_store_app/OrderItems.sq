CREATE TABLE order_items (
    order_no INTEGER PRIMARY KEY AUTOINCREMENT,
    title TEXT NOT NULL,
    price REAL NOT NULL,
    qty INTEGER NOT NULL,
    order_id TEXT NOT NULL

);

insertOrder:
INSERT INTO order_items( title, price, qty , order_id)
VALUES ( ?, ?, ? , ?);

selectAll:
SELECT * FROM order_items;
getMaxSlipNo:
SELECT MAX(order_id) FROM order_items;
